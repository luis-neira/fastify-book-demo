{
  "type": "object",
  "$id": "schema:dotenv",
  "required": ["MONGO_URL"],
  "properties": {
    "NODE_ENV": {
      "type": "string",
      "default": "development"
    },
    "PORT": {
      "type": "integer",
      "default": 3000
    },
    "MONGO_URL": {
      "type": "string"
    },
    "MONGO_INITDB_ROOT_USERNAME": {
      "type": "string"
    },
    "MONGO_INITDB_ROOT_PASSWORD": {
      "type": "string"
    }
  }
}
